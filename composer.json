{
    "name": "fontis/australia-mage2",
    "description": "Magento extension that provides functionality for Australian stores.",
    "type": "magento2-module",
    "license": "OSL-3.0",
    "require": {
        "php":  "7.3.*|~7.4.0",
        "vuthuan/auspost-api-php": "~2.0.3",
        "vuthuan/bpay-ref-generator": "~1.0.4",
        "vuthuan/customergroupaccess-mage2": "~1.0.7",
        "magento/framework": "100.0.*|100.1.*|101.0.*|102.0.*|103.0.*",
        "magento/module-checkout": "100.0.*|100.1.*|101.0.*|102.0.*|103.0.*",
        "magento/module-directory": "100.0.*|100.1.*|101.0.*|102.0.*|103.0.*",
        "magento/module-payment": "100.0.*|100.1.*|101.0.*|102.0.*|103.0.*",
        "magento/module-store": "100.0.*|100.1.*|101.0.*|102.0.*|103.0.*",
        "magento/module-shipping": "100.0.*|100.1.*|101.0.*|102.0.*|103.0.*"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Fontis\\Australia\\": ""
        }
    },
    "repositories": {
        "auspost-api-php": {
                "type": "vcs",
                "url": "https://github.com/vuthuan/auspost-api-php.git"
        },
        "bpay-ref-generator": {
                "type": "vcs",
                "url": "https://github.com/vuthuan/bpay-ref-generator.git"
        },
        "customergroupaccess-mage2": {
                "type": "vcs",
                "url": "https://github.com/vuthuan/customergroupaccess-mage2.git"
        }
    }
}
